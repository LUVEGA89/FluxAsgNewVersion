        $("#td-bancomer-comentario-" + Index).text(Comentario);
        $("#td-bancomer-estatus-" + Index).text("Modificado");

        $.ajax({
            contentType: "application/json; charset=utf-8",
            url: '@Url.Action("GetEstadoBancomerBySequence", "Estados")',
            async: false,
            timeout: 3000,
            type: 'POST',
            data: '{"Sequence":"' + Sequence + '"}',
            dataType: "json",
            success: function (data) {
                let bancomer_abono_msg = data.Context[0].Abono;
