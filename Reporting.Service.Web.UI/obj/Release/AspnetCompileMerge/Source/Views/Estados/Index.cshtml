<script type="text/javascript">
    //Formato de dinero
    var formatNumber = {
        separador: ",",
        sepDecimal: '.',
        formatear: function (num) {
            num += '';
            var splitStr = num.split('.');
            var splitLeft = splitStr[0];
            var splitRight = splitStr.length > 1 ? this.sepDecimal + splitStr[1] : '';
            var regx = /(\d+)(\d{3})/;
            while (regx.test(splitLeft)) {
                splitLeft = splitLeft.replace(regx, '$1' + this.separador + '$2');
            }
            return this.simbol + splitLeft + splitRight;
        },
        new: function (num, simbol) {
            this.simbol = simbol || '';
            return this.formatear(num);
        }
    }

    function getEstadoBancomer() {
        $('#loading-steuben-bancomer').show();
        $.ajax({
            contentType: "application/json; charset=utf-8",
            url: '@Url.Action("GetEstadoBancomer", "Estados")',
            async: false,
            timeout: 3000,
            type: 'POST',
            data: '{}',
            dataType: "json",
            success: function (data) {
                if (data != null && $.isArray(data.Context)) {
                    var VP = $('#table-steuben-bancomer').DataTable();
                    VP.destroy();

                    $("#tbody-steuben-bancomer").html('');
                    $.each(data.Context, function (index, value) {
                        if (value.Estatus == 1) {
                            value.Estatus = 'Vacio'
                        }
                        if (value.Estatus == 2) {
                            value.Estatus = 'Modificado'
                        }
                        if (value.Estatus == 3) {
                            value.Estatus = 'Confirmado'
                        }
                        $("#tbody-steuben-bancomer").append(
                            "<tr>\
                                <td>" + value.Sequence + "</td>\
                                <td>" + value.Fecha + "</td>\
                                <td>" + value.Referencia + "</td>\
                                <td> " + formatNumber.new(value.Abono, "$ ") + "</td >\
                                <td style='display:none'>" + value.Abono + "</td>\
                                <td id='td-bancomer-sucursal-"+index+"'>" + value.Sucursal + "</td>\
                                <td id='td-bancomer-tipo-pago-"+ index +"'>" + value.TipoPago + "</td>\
                                <td id='td-bancomer-fecha-venta-"+ index +"'>" + value.FecDiaVenta + "</td>\
                                <td id='td-bancomer-comentario-"+ index + "'>" + value.Comentario + "</td>\
                                <td id='td-bancomer-estatus-"+ index +"' > " + value.Estatus + "</td >\
                                <td><a class='btn btn-info' data-target='#myModalSteubenBancomer' data-toggle='modal' data-id='" + value.Sequence + "' data-index-id='" + index + "' href='#'><i class='fa fa-exchange'></i></a></td></tr>");
                    }); 

                    $('#table-steuben-bancomer').DataTable({
                        "lengthMenu": [[10, 20, 30, -1], [10, 20, 30, "All"]],
                        dom: 'Bfrtip',
                        stateSave: true,
                        buttons: [
                            'excel', 'pdf'
                        ],
                        "paging": true,
                        "searching": true,
                        "language": {
                            "url": "//cdn.datatables.net/plug-ins/1.10.13/i18n/Spanish.json"
                        }
                    });
                }
            },
            error: function () {
                alert("Error en la api: GetEstadoBancomer");
            }
        });
        $('#loading-steuben-bancomer').hide();
    }
    getEstadoBancomer();
    $('#search-input-bancomer').keyup(function () {
        search_table_bancomer($(this).val());
    });
    function search_table_bancomer(value) {
        $('#tbody-steuben-bancomer tr').each(function () {
            var found_bancomer = 'false';
            $(this).each(function () {
                if ($(this).text().toLowerCase().indexOf(value.toLowerCase()) >= 0) {
                    found_bancomer = 'true';
                }
            });
            if (found_bancomer == 'true') {
                $(this).show();
            }
            else {
                $(this).hide();
            }
        });
    } 
    var rows = $('#tbody-steuben-bancomer tr').length;    
    $("#count-tr-bancomer").text(" --- "+rows+" registros");
    //HASTA AQUI ACABA BANCOMER--------------------------------------------------
    function getEstadoBanorte() {
        $.post('@Url.Action("GetEstadoBanorte", "Estados")', {
        }, function (data) {
            if (data != null && $.isArray(data.Context)) {
                $.each(data.Context, function (index, value) {
                    if (value.Estatus == 1) {
                        value.Estatus = 'Vacio'
                    }
                    if (value.Estatus == 2) {
                        value.Estatus = 'Modificado'
                    }
                    if (value.Estatus == 3) {
                        value.Estatus = 'Confirmado'
                    }
                    $("#tbody-banorte-steuben").append(
