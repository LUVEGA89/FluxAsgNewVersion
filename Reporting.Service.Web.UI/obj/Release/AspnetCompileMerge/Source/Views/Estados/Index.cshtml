                        dataType: "json",
                        success: function (data) {
                            localStorage.removeItem("bancomer_sequence_msg");
                        },
                        error: function () {
                            alert("Error en la api: ActualizarEstadoDeCuentaSteubenBancomer");
                        }
                    });
                });
            } else {
                localStorage.removeItem("storage_abono");
                swal("Cancelado", "Operación cancelada", "error");
            }
        });
    });

