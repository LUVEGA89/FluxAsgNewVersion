    function initModule() {

        // Combos
        $("#select-tipo-documento option:eq(0)").prop("selected", true);
        $("#select-concepto-descuento option:eq(0)").prop("selected", true);

        // divs
        $('#nota-credito-inventario').hide();
        $('#nota-credito-servicio-otro-descuento').hide();
        $('#nota-credito-servicio').hide();

        // div concepto descuento
        $('#div-concepto-descuento').hide();

