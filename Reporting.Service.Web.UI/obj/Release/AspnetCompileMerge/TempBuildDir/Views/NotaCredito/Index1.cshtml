Usua@model Reporting.Service.Web.UI.Models.UserModel
@using Microsoft.AspNet.Identity
@using Reporting.Service.Core.Common
@{
    ViewBag.Title = "Notas de Credito";
}
<style>  

    .span-help-block-monto-ocupado {
        color: #FF0000 !important;
    }

    .span-help-block-monto-disponible {
        color: #0000cc !important;
    }

    #estatus-rechazado {
        text-align: center;
        color: red;
    }

    #select-concepto-descuento option[value="37"] {
        background: yellow;
    }

    #select-concepto-descuento option[value="39"] {
        background: yellow;
    }

    .meta-date-gestion {
        background: #F2635F !important
    }

</style>

<section class="content-header">
    <h1>
        Solicitud de Nota de Crédito
        <small id="span-cliente"></small>
        <small id="span-cliente-canal"></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Nota de Crédito</a></li>
        <li class="active">Solicitud</li>
        <li class="active">GZ</li>
    </ol>
</section>
<section class="content">
    <div class="row">

        <div class="col-md-4" id="cabecera">
            <div class="box box-danger">

                <div id="overlay-creditnote-solicitud" class="overlay" style="display:none;">
                    <i class="fa fa-refresh fa-spin"></i>
                </div>

                <div class="box-header">
                    <div class="box-title">
                        <i class="fa fa-search"></i>
                        Motór de busqueda
                    </div>
                </div>

                <div class="box-body">

                    <form role="form">

                        <div class="form-group">
                            <label for="">Cliente</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <span class="fa fa-search"></span>
                                </div>
                                <input type="text" class="form-control" id="input-cliente" placeholder="Nombre / Codigo" data-behavior="autocomplete" value="" required>
                            </div>
                        </div>

                        <div id="div-factura-origen" class="form-group">
                            <label>Factura origen</label>
                            <input id="input-factura-origen" placeholder="Factura origen" type="text" onkeypress="return isNumber(event)" class="form-control">
                            <span id="spam-factura-origen" class="help-block"></span>
                        </div>

                        <div id="div-factura-destino" class="form-group">
                            <label>Factura destino</label>
                            <input id="input-factura-destino" placeholder="Factura destino" onkeypress="return isNumber(event)" type="text" class="form-control">

                            <span id="spam-factura-destino" class="help-block"></span>
                            <span id="spam-factura-destino-ocupado" class="help-block span-help-block-monto-ocupado"></span>
                            <span id="spam-factura-destino-disponible" class="help-block span-help-block-monto-disponible"></span>

                        </div>

                        <div class="form-group">
                            <label>Tipo de documento</label>
                            <select class="form-control" id="select-tipo-documento">
