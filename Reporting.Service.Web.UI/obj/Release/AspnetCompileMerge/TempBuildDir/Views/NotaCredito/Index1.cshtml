    @* GUARDAR LA NOTA DE CREDITO SERVICIO *@
    $('#button-guardar-nota-credito-servicio').click(function (e) {
        e.preventDefault();

        @*AGREGAR LA NOTA DE CREDITO ITEM*@
        console.log('Length array ' + NotaCreditoItems.length);
        if (NotaCreditoItems.length > 0) {
            NotaCreditoItems = [];            
        }

        let MontoSolicitado = $('#input-factura-destino-monto-solicitado-servicio').val();
        if (parseInt(MontoSolicitado) == 0) {
            swal("Monto permitido", "El monto pemitido debe ser mayor a 0 para agregar la nota de credito", "warning");
            return false;
        }
        let CuentaContable = $('#input-cuenta-contable-servicio-descuento-hidden').val();
        //console.log('CuentaContable ' + CuentaContable);
        if (CuentaContable == '') {
            swal("Cuenta contable", "La cuenta contable no se ha definido para guardar la nota de crédito", "warning");
            return false;
        }

        let Canal = $('#span-cliente-canal').text();
        if (Canal == '') {
            swal('Canal', 'No se ha definido el canal del cliente actual', 'warning');
            return false;
        }

        let concepto_descuento = $('#select-concepto-descuento').val();
        if (concepto_descuento == '0') {
            swal('Concepto de descuento', 'El concepto de descuento seleccionado no es valido para generar la nota de crédito', 'warning');
            return false;
        }

        let MontoPermitido = 0;
        if ((Canal == 'RETAIL') || (Canal == 'MAYOREO' && concepto_descuento == 39) || (Canal == 'MAYOREO' && concepto_descuento == 93)) {
            MontoPermitido = $('#input-factura-destino-monto-permitido-servicio-hidden').val();
            //console.log('MontoPermitido origen ' + MontoPermitido);
        }
        else {
            MontoPermitido = $('#input-factura-destino-monto-permitido-servicio-disponible-hidden').val();            
        }

        if (parseInt(MontoPermitido) == 0 || parseFloat(MontoPermitido) == 0.00) {
            swal("Monto solicitado", "El monto pemitido debe ser mayor a 0 para agregar la nota de credito verificar la nota de credito ingresada los datos sean correctos", "warning");
            return false;
        }

        let ConceptoDescuentoText = $("#select-concepto-descuento option:selected").text();

        let EmailUser = localStorage.getItem('UserEmail');
        @* AGREGAR LA PARTIDA DE LA NOTA DE CREDITO*@
        AddNotaCreditoItem(ConceptoDescuentoText, "1", MontoPermitido, MontoSolicitado, CuentaContable);
        console.log('Array items: ' + JSON.stringify(NotaCreditoItems));

        if (NotaCreditoItems.length == 0) {
            swal("Partida vacia", "Debe de seleccionar una partida de la nota de credito", "warning");
            return false;
        }

        let Cliente = $('#input-cliente').val();
        if (Cliente == '') {
            swal('Cliente', 'Debe de especificar un cliente para guardar la nota de crédito', 'warning');
            return false;
        }

        let FolioOrigen = $('#input-factura-origen').val();
        if (FolioOrigen == '') {
            swal('Factura origen', 'Debe de especificar la factura origen para guardar la nota de crédito', 'warning');
            return false;
        }

        let FolioDestino = $('#input-factura-destino').val();
        if (FolioDestino == '') {
            swal('Factura destino', 'Debe de especificar la factura destino para guardar la nota de crédito', 'warning');
            return false;
        }


        if (Canal == 'NO') {
            swal('Cliente', 'El cliente no tiene definido un canal para generar la nota de crédito', 'warning');
            return false;
        }
        let tipo_documento = $("#select-tipo-documento option:selected").val();
        if (tipo_documento == 0) {
            swal('Tipo de documento', 'El tipo de documento seleccionado no es valido para generar la nota de crédito', 'warning');
            return false;
        }

        let Comentario = $('#comentario').val();



        swal({
            title: "¿Solicitar nota de credito?",
            text: "Se mandará solicitud de la nota de crédito Servicio " + ConceptoDescuentoText + ".",
            icon: "warning",
            buttons: [
              'No, cancelar esta operación',
              'Si, estoy seguro'
            ],
            dangerMode: true,
        }).then(function (isConfirm) {
            if (isConfirm) {
                swal({
                    title: 'Operación confirmada',
                    text: 'Enviando datos',
                    icon: 'success'
                }).then(function () {
                    let EmailUser = localStorage.getItem('UserEmail');

                    let ConceptoDescuentoText = $("#select-concepto-descuento option:selected").text();
                    //alert("Concepto descuento " + ConceptoDescuentoText);

                    $.post('@Url.Action("AddNotaCredito", "NotaCredito")',
                    {
                        Cliente: Cliente,
                        FolioOrigen: FolioOrigen,
                        FolioDestino: FolioDestino,
                        FolioDestino: FolioDestino,
                        TipoDocumento: tipo_documento,
                        Comentario : Comentario,
                        Usuario: EmailUser,
                        ConceptoDescuento: concepto_descuento,
                        Canal: Canal,
                        ClienteName: $('#span-cliente').text(),
                        ConceptoDescuentoDetalle : ConceptoDescuentoText,
                        Items: NotaCreditoItems
                    }, function (data) {
                        if (data.Code == '-1' || concepto_descuento == '0') {
                            initModule();
                            getHistoricoNotaCredito();
                            swal("Nota de crédito","Favor de verificar la logistica del descuento.", "warning");
                        }                        
                        else if (data.Context != null) {                                                        
                            if (data.Context) {
                                swal("Nota de crédito", "Nota de crédito agregado", "success");
                                @* ENVIA EL CORREO AL USUARIO QUE GENERO LA NOTA DE CREDITO*@
                                initModule();
                                getHistoricoNotaCredito();
                            }
                            else
                            {
                                swal("Nota de crédito", "Ocurrio un error inesperado para agregar la nota de credito", "error");
                                initModule();
                            }                            
                        }
                        else {
                            if (data.Message != '') {
                                swal("Nota de crédito", "Mensaje " + data.Message, "error");
                                initModule();
                            }
                            
                            
                        }
                    }).done(function (data) {
                    }).fail(function (xhr, textStatus, errorThrown) {
                        console.log('xhr' +  JSON.stringify(xhr));
                        console.log('textStatus' + JSON.stringify(textStatus));
                        console.log('errorThrown ' + JSON.stringify(errorThrown));                        
                    }).always(function (data) {                        
                    });


                });
            } else {
                NotaCreditoItems = [];
                initModule();
                swal("Aviso de nota de crédito", "La nota de crédito se ha cancelado.", "error");
            }
        });
    });


