    function GetNotaCreditoInventario() {
        // folio de la factura origen
        let FOrigen = $('#input-factura-origen').val();
        let count = 0;

        $.ajax({
            contentType: "application/json; charset=utf-8",
            url: '@Url.Action("GetNotaCreditoInventario", "NotaCredito")',
            async: false,
            timeout: 3000,
            type: 'POST',
            data: '{"FacturaOrigen":"' + FOrigen + '","TipoDocumento":"I"}',
            dataType: "json",
            success: function (data) {

                if (data != null && $.isArray(data.Context)) {

                    // "#table-invoice-history"
                    var VP = $('#factura-origen').DataTable();
                    VP.destroy();
                    $("#table-invoice-history").html('');

                    count = data.Context.length;

                    //console.log("Notas de credito encontradas " + count);

                    $.each(data.Context, function (index, value) {
                        let EstadoAprobacion = '';

                        if (value.TipoDocumento == 'S') {
                            if (value.Valor == 1 && value.Canal == 'RETAIL') {

                                EstadoAprobacion = 'Aprobación pendiente por credito y cobranza';
                            }
                            else {
                                EstadoAprobacion = 'Aprobación pendiente por dirección';
                            }
                            value.TipoDocumento = 'Servicio';
                        }

                        if (value.TipoDocumento == 'I') {
                            value.TipoDocumento = 'Inventario';
                            if (value.Estatus == 1) {
                                value.Estatus = 'Aprobación pendiente por el almacén'
                            }
                        }

                        if (value.Valor == 0) {
                            if (value.ConceptoDescuento == 39) {

                                EstadoAprobacion = 'Aprobación pendiente por credito y cobranza';
                            }
                            else {
                                EstadoAprobacion = 'Aprobación pendiente por gerencia';
                            }
                        }

                        if (value.Valor == 2) {
                            EstadoAprobacion = 'Aprobación pendiente por la dirección'
                        }
                        if (value.Valor == 3) {
                            EstadoAprobacion = 'Autorizada'
                        }
                        if (value.Valor == 4) {
                            EstadoAprobacion = 'SAP'
                        }
                        if (value.Valor == 5) {
                            EstadoAprobacion = 'Rechazada';
                        }
                        if (value.Valor == 10) {
                            EstadoAprobacion = 'No especificado';
                        }
                        $("#table-invoice-history").append(
                            "<tr>\
                                <td>" + value.Identifier + "</td>\
                                <td>" + value.ClienteName + "</td>\
                                <td>" + value.FolioOrigen + "</td>\
                                <td>" + value.FolioDestino + "</td>\
                                <td>" + value.TipoDocumento + "</td>\
                                <td>" + value.ConceptoDescuentoDetalle + "</td>\
                                <td>" + value.Comentario + "</td>\
                                <td>" + value.Usuario + "</td>\
                                <td>" + EstadoAprobacion + "</td>\
                                <td>" + moment(value.Fecha).format("YYYY-MM-DD HH:mm:ss") + "</td>\
                                <td>" + value.FolioSap + "</td>\
                                <td>" + value.FolioPagoSAP + "</td>\
                                <td align='center'>\
                                    <a class='btn btn-primary' data-target='#modal-nota-credito-inventario' data-toggle='modal' onclick='getHistoricoNotaCreditoDetail("+ value.Identifier + ")' href= '#'>\
                                        <i class='fa fa-file-text'></i>\
                                    </a>\
                                </td>\
                                <td align='center'>\
                                    <a class='btn btn-info' data-target='#verComentarios' data-toggle='modal' onclick='verComentarios("+ value.Identifier + ")' data-id='" + value.Identifier + "' href= '#'>\
                                        <i class='fa fa-comment'></i>\
                                    </a>\
                                </td>\
                                <td align='center'>\
                                    <a class='btn btn-info' data-target='#verImagenes' data-toggle='modal' onclick='verImagenesNC("+ value.Identifier + ")' data-id='" + value.Identifier + "' href= '#'>\
                                        <i class='fa fa-eye'></i>\
                                    </a>\
                                </td>\
                                </tr>");
                    });

                    $('#factura-origen').DataTable({
                        "lengthMenu": [[10, 20, 30, -1], [10, 20, 30, "All"]],
                        dom: 'Bfrtip',
                        stateSave: true,
                        buttons: [
                            'excel'
                        ],
                        order: [[1, 'desc']],
                        "paging": true,
                        "searching": true,
                        "language": {
                            "url": "//cdn.datatables.net/plug-ins/1.10.13/i18n/Spanish.json"
                        }
                    });
                }
                if (count > 0) {
                    swal("Verificar notas de credito aplicadas en el historial a la factura actual: " + FOrigen + "");
                }

            },
            error: function () {
                swal("Error en la api: GetNotaCreditoInventario");
            }
        });
    }


    let count1 = 1;
    @*CLONA EN TABLA VALORES DE LA NOTA DE CREDITO TIPO INVENTARIO*@
    function clonarTablaNotaDeCreditoInventario(Codigo, Precio, index, AcctCode, Cantidad){

        let exist = 0;
        // count list
        let count = $(".sku_inventario");

        if (count.length > 0) {
            var aux = 0;

            $(".sku_inventario").each(function () {
                let sku = this.value;                
                let cant1 = $("#cantidad-inventario-" + aux).val();

                if (Codigo == sku && cant1 == Cantidad) {
                    exist = 1;
                    swal("Registro duplicado", "El SKU " + Codigo + " ya se ha agregado anteriormente.", "warning");
                    return false;
                }
                aux++;
            });
        }
        if (exist == 0) {
            $('#box-inventario-clonado').show();

            $("#tr-invoice-" + index).css("background-color", "#e6ffe6");

            $("#tabla-inventario-clone").show();
            // add array item credit note

            Cantidad = $("#cantidad-inventario-" + index).val();

            let SubTotal = Cantidad * Precio;

            let subFormat = formatNumber.new(SubTotal.toFixed(3), "$ ");
            let preFormat = formatNumber.new(parseFloat(Precio).toFixed(3), "$ ");

            count1 = count1 + 1;
            var html_code = "";
            html_code += "<tr id='row" + count1 + "'>";
            html_code += "<td><input class='sku_inventario' type='text' value='" + Codigo + "' disabled/></td>";
            html_code += "<td><input class='cantidad-inventario' type='text' style='width: 100px;' value='" + Cantidad + "' disabled/></td>";
            html_code += "<td><input type='text' style='width: 100px;' value='" + preFormat + "' disabled/></td>";
            html_code += "<td><input type='text' value='" + subFormat + "' disabled/></td>";
            html_code += "<td style='display:none'><input type='hidden' class='subtotal-hidden' value='" + SubTotal + "'/></td>";
            html_code += "<td><button type='button' name='remove' data-row='row" + count1 + "' onclick='test(\"" + index + "\",\"" + Codigo + "\")' class='btn btn-danger btn-sm remove'><i class='fa fa-trash'></i></button></td>";
            html_code += "</tr>";

            $("#tabla-inventario-clone").append(html_code);

            @* MONTO SOLICITADO *@
            let monto_permitido = $("#factura-inventario-destino-monto-permitido").val();

            let Sum = 0;
            $(".subtotal-hidden").each(function () {
                let sumado = parseFloat(this.value);
                if (!isNaN(sumado) && sumado.length != 0) {
                    Sum += sumado;
                }
            });
            let IVA = Sum * 0.16;
            let Total = Sum + IVA;
            let code = "";

            //$('#tabla-inventario-clone').append(code);

            $('#subtotal-inventario-clonado').text(formatNumber.new(Sum.toFixed(3), "$ "));
            $('#iva-inventario-clonado').text(formatNumber.new(IVA.toFixed(3), "$ "));
            $('#total-inventario-clonado').text(formatNumber.new(Total.toFixed(3), "$ "));


            if (parseFloat(Total) > parseFloat(monto_permitido) + 5) {
                swal({
                    title: "Solicitud",
                    text: "El monto de la nota de credito original no puede ser superado.",
                    icon: "error",
                    button: "OK",
                });
                $('#row' + count).remove();

                @* REMOVE ITEM DE LA NOTA DE CREDITO *@
                RemoveItemCreditNote(Codigo);

                Sum = 0; IVA = 0; Total = 0;

                $(".subtotal-hidden").each(function () {
                    let sumado = parseFloat(this.value);
                    if (!isNaN(sumado) && sumado.length != 0) {
                        Sum += sumado;
                    }
                });
                IVA = Sum * 0.16;
                Total = Sum + IVA;
                $('#subtotal-inventario-clonado').text(formatNumber.new(Sum.toFixed(3), "$ "));
                $('#iva-inventario-clonado').text(formatNumber.new(IVA.toFixed(3), "$ "));
                $('#total-inventario-clonado').text(formatNumber.new(Total.toFixed(3), "$ "));
                return false;
            }
            else if (Total == monto_permitido) {
                swal({
                    title: "Solicitud",
                    text: "Nota de credito (inventario) completada.",
                    icon: "success",
                    button: "OK",
                });
            }

            AddNotaCreditoItem(Codigo, Cantidad, Precio, "0", AcctCode);
            console.log("Array add item: " + JSON.stringify(NotaCreditoItems));


        }

    }

