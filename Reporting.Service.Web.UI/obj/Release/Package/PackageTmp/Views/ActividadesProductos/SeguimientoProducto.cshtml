<!---------------------- Manejo del modal Add Nuevo Producto --------------------------------->
<script>
    $("#linkAddNuevoProducto").click(function (e) {
        e.preventDefault();
        LimpiarModalNuevoProducto();
        $("#modal-NuevoProducto").modal('show');
    });
    function LimpiarModalNuevoProducto() {
        $("#TxtCodeSKU").val('');
        $("#TxtNameSKU").val('');
        $("#txt-EmpaqueNuevoProducto").val('default');
        $("#txt-MarcaNuevoProducto").val('default');
        $("#TxtCategoria").val('default');
        $("#TxtSubCategoria").val('default');
        $("#tbPrecioNuevoProducto").html('');
        $("#TxtCodeSKU").focus();
        $("#imagenProductoNuevo").val('');
    }
    $("#btn-NuevoProducto-cancel").click(function () {
        LimpiarModalNuevoProducto();
        $("#modal-NuevoProducto").modal('hide');
    });
    $("#TxtCategoria").change(function () {
        var Id = $("#TxtCategoria").val();
        $.post('@Url.Action("GetCategoriasSKU", "ActividadesProductos")', {
            Id: Id
        }, function (data) {
            $("#TxtSubCategoria").html('');
            if (data.Context.length <= 0 || data.Context === null) {
                $("#TxtSubCategoria").append(
                    "<option value='0' selected>No contiene Subcategoria</option>"
                );
            } else {
                $("#TxtSubCategoria").append(
                    "<option value='default' selected disabled>Seleccione un Subcategoria</option>"
                );
                $.each(data.Context, function (index, value) {
                    $("#TxtSubCategoria").append(
                        "<option value='" + value.Id + "'>" + value.Name + "</option>"
                    );
                });
            }
        });
    });
    $("#btn-NuevoProducto-add").click(function () {
        var CodeSKU = $("#TxtCodeSKU").val();
        var NameSKU = $("#TxtNameSKU").val();
        var MarcaProducto = $("#txt-MarcaNuevoProducto").val();
        var Categoria = $("#TxtCategoria").val();
        var Empaque = $("#txt-EmpaqueNuevoProducto").val();

        //******  control de validaciones  ******//
        if (CodeSKU == "") {
            swal("Alerta", "Debe ingresar un código al SKU.", "warning");
            $("#TxtCodeSKU").focus();
            return;
        }
        if (NameSKU == "") {
            swal("Alerta", "Debe ingresar un nombre al SKU.", "warning");
            $("#TxtNameSKU").focus();
            return;
        }
        if (Empaque == "" || Empaque === null) {
            swal("Alerta", "Debe seleccionar una Tipo de Empaque.", "warning");
            $("#txt-EmpaqueNuevoProducto").focus();
            return;
        }
        if (MarcaProducto == "" || MarcaProducto === null) {
            swal("Alerta", "Debe seleccionar una marca.", "warning");
            $("#txt-MarcaNuevoProducto").focus();
            return;
        }
        if (($("#imagenProductoNuevo").get(0).files.length) <= 0) {
            swal("Alerta", "Debe ingresar imagen de producto", "warning");
            $("#imagenProductoNuevo").focus();
            return;
        }
        if (($("#imagenProductoNuevo").get(0).files.length) > 5) {
            swal("Alerta", "Debe seleccionar un máximo de 5 imagenes.", "warning");
            $("#imagenProductoNuevo").focus();
            return;
        }
        if ($('#tbPrecioNuevoProducto td').length <= 0) {
            swal("Alerta", "Debe agregar precio al producto nuevo.", "warning");
            return;
        }
        if (Categoria == "") {
            swal("Alerta", "Debe Seleccionar una Categoria.", "warning");
            return;
        }
        ShowOver();
        var formData = new FormData($("#frm-NuevoProducto")[0]);
        var Ruta = "@Url.Action("GuardarProductoNuevo", "ActividadesProductos")";
        $.ajax({
            url: Ruta,
            type: "POST",
            data: formData,
            contentType: false,
            processData: false,
            success: function (data) {
                if (data.Context.length <= 0 || typeof data.Context === null) {
                    HideOver();
                } else {
                    var ProductosSKU = { preciosCompetencias: [] };
                    var Ruta2 = "@Url.Action("GuardarPrecioCompetencia", "ActividadesProductos")";
                    var tamTabla = $('#TablePrecioNuevoProducto td').length;
                    var cont = 0;
                    for (var i = 0; i < (tamTabla / 3); i++) {
                        ProductosSKU.preciosCompetencias.push({
                            "IdSKU": data.Context,
                            "TipoPrecio": $('#TablePrecioNuevoProducto').find('td').eq(cont).html(),
                            "NumPiezas": $('#TablePrecioNuevoProducto').find('td').eq(cont + 1).html(),
                            "Precio": $('#TablePrecioNuevoProducto').find('td').eq(cont + 2).html(),
                        });
                        cont = cont + 3;
                    }
                    var JsonProductosSKU = JSON.stringify(ProductosSKU);
                    if (tamTabla > 0) {
                        $.ajax({
                            url: Ruta2,
                            type: "POST",
                            data: JsonProductosSKU,
                            contentType: 'application/json; charset=utf-8 ',
                            dataType: 'JSON',
                            success: function (response) {
                                enviarCorreoNuevoSKU(CodeSKU);
                                $("#modal-NuevoProducto").modal('hide');
                                LimpiarModalNuevoProducto();
                                HideOver();
                            }
                        });
                    } else {
                        enviarCorreoNuevoSKU(CodeSKU);
                        $("#modal-NuevoProducto").modal('hide');
                        LimpiarModalNuevoProducto();
                        HideOver();
                    }

                }
            }
        });
    });
    $("#btn-addTbProductoNuevo").click(function () {
        LimpiarModalAddTabla();
        $("#modalTablaPrecioProduto").modal('show');
    });
    /******************* Manejo de modal add precios a tabla ***************************************/
    function LimpiarModalAddTabla() {
        $("#TxtTipoPrecio").val('default');
        $("#TxtNumPiezas").attr('readonly', false);
        $("#TxtNumPiezas").val('');
        $("#TxtPrecioPieza").val('');
    }
    function TipoPrecioEvento() {
        var aux = $("#TxtTipoPrecio").val();
        if (aux == "Público General") {
            $("#TxtNumPiezas").val('1');
            $("#TxtNumPiezas").attr('readonly', true);
        } else {
            $("#TxtNumPiezas").attr('readonly', false);
            $("#TxtNumPiezas").val('');
        }
    }
    //************ VAlidacion de campos *********//
    $("#TxtNumPiezas").keyup(function () {
        $(this).val($(this).val().replace(/[^0-9]/g, ""));
    });
    $("#TxtPrecioPieza").keyup(function () {
        var ExpReg = /(0-9\.{1}|0-9\.{1}0-9{2})?/;
        var valor = $(this).val();
        if ((valor.split(".").length - 1) > 1) {
            $(this).val($(this).val().replace(".", "/"));
            $(this).val($(this).val().replace(".", ""));
            $(this).val($(this).val().replace("/", "."));
        } else {
            $(this).val($(this).val().replace(/[^\d+\.{1}\d{2}]/, ""));
        }
    });

    $("#btnAddPrecioProducto").click(function () {
        var TipoPrecio = $("#TxtTipoPrecio").val();
        var NumeroPiezas = $("#TxtNumPiezas").val();
        var PrecioXPieza = $("#TxtPrecioPieza").val();

        if (TipoPrecio == "" || TipoPrecio === null) {
