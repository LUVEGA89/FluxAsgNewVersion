
@{
    ViewBag.Title = "Seguimiento Producto";
}

@model Reporting.Service.Web.UI.Models.ProductosModel
@{
    var tam = this.Model.Roles.Count();
    var roles = this.Model.Roles;
}

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-treeview/1.2.0/bootstrap-treeview.min.css" />
<script type="text/javascript" charset="utf8" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-treeview/1.2.0/bootstrap-treeview.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/2.1.2/sweetalert.min.js">
<script src="https://cdn.jsdelivr.net/npm/promise-polyfill@8/dist/polyfill.js"></script>
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.0/css/all.css" integrity="sha384-aOkxzJ5uQz7WBObEZcHvV5JvRW3TUc2rNPA7pe3AwnsUohiw1Vj2Rgx2KSOkF5+h" crossorigin="anonymous">
<style>
    .widget-user .box-footer {
        padding-top: 100px !Important;
    }

    .carousel-inner > .item > a > img, .carousel-inner > .item > img, .img-responsive, .thumbnail a > img, .thumbnail > img {
        display: block;
        width: 100%;
        height: 150px;
    }

    #imagenDetalle {
        width: 50%;
        margin-left: 25%;
    }

    #carousel-example-generic1 {
        width: 50%;
    }

    .btn-spacing {
        margin-right: 10px;
    }

    #panelFind {
        max-height: 200px;
        overflow-y: scroll;
    }

    #panelNuevoProducto {
        height: 150px;
        max-height: 200px;
        overflow-y: scroll;
    }
    /*#tbPrecioNuevoProducto {
        height: 150px;
        max-height: 200px;
        overflow-y: scroll;
    }*/

    .table {
        margin-top: 3px;
    }

    .table, thead {
        background-color: rgba(121, 119, 121,0.2);
        color: black;
        border: 1px solid white;
    }

    .table, tbody {
        border: 1px solid white;
    }

    #addTablePrecios thead {
        background-color: rgba(0,92,191,0.9);
        color: white;
    }

    #tbSKUSugerido {
        width: 50%;
        text-align: center;
    }

        #tbSKUSugerido thead {
            background-color: rgba(0,92,191,0.9);
            color: white;
        }

        #tbSKUSugerido tbody {
            background-color: lightgray;
            color: black;
        }

    #modal-success {
        width: 100%;
        height: 800px;
    }

    #tb-Comparacion {
        padding: 0px;
        max-height: 150px;
        overflow-y: scroll;
    }

    #imagenEvidencia {
        padding: 0px;
        max-height: 130px;
        overflow-y: scroll;
    }
    /************* Radios de seleccion de busqueda de productos *************/
    #tbRadios {
        background-color: rgb(42,131,200);
        color: white;
    }

        #tbRadios tr th {
            color: white;
            text-align: center;
            width: 50%;
        }
    /************* Modal de nuevos productos *************/

    #modal-NuevoProducto .modal-body {
        padding-top: 0px;
        padding-bottom: 0px;
    }

    #modal-NuevoProducto .modal-footer {
        padding-top: 5px;
        padding-bottom: 5px;
    }
    /************* Modal de Imagenes *********************/
    #imagemodal {
        display: none;
        position: fixed;
        z-index: 9999;
        padding-top: 2%;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        overflow: auto;
        background-color: rgba(0,0,0,0.9);
    }

        #imagemodal .close {
            color: white;
            font-size: 20px;
            font-weight: bold;
            transition: 0.3s;
        }

            #imagemodal .close:hover,
            #imagemodal .close:focus {
                color: white;
                text-decoration: none;
                cursor: pointer;
            }

        #imagemodal .modal-content {
            background-color: rgba(0,0,0,0);
            margin: auto;
            display: block;
            width: 400px;
        }

        #imagemodal .modal-content {
            -webkit-animation-name: zoom;
            -webkit-animation-duration: 0.6s;
            animation-name: zoom;
            animation-duration: 0.6s;
        }

    @@-webkit-keyframes zoom {
        from {
            -webkit-transform: scale(0)
        }

        to {
            -webkit-transform: scale(1)
        }
    }

    @@keyframes zoom {
        from {
            transform: scale(0)
        }

        to {
            transform: scale(1)
        }
    }

    @@media only screen and (max-width: 700px) {
        #imagemodal .modal-content {
            width: 100%;
        }
    }
    /*carga de imagenes, loader de espera*/
    /*.loader {
        position: fixed;
        left: 0px;
        top: 0px;
        width: 100%;
        height: 100%;
        z-index: 9999;
        opacity: .7;
        background-color: rgb(14,30,47);
        background: url('../../Content/dist/img/pageLoader.gif') 50% 50% no-repeat rgb(14,30,47);
    }*/
    /*** diseño de tree de seguimiento de productos nuevos */
    /*#treeviewSKUFechasNuevo .list-group .list-group-item .node-treeviewSKUFechasNuevo:active {
        background-color:red;

    }*/
    #PanelSeguimientoProductos #chatSeguimientoFecha {
        min-height: 200px;
    }

    #PanelSeguimientoProductos #chat-box {
        min-height: 200px;
    }

    #PanelSeguimientoProductos #chatSeguimientoFechaNuevoSKU {
        min-height: 200px;
    }

    #overBody {
        position: fixed;
        left: 0px;
        top: 0px;
        width: 100%;
        height: 100%;
        z-index: 9999;
        opacity: .7;
        background-color: rgb(14,30,47);
    }
</style>
<!--cabecera de pagina-->
<section class="content-header">
    <h1>
        Seguimiento de productos
        <small>Panel de control</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Seguimiento de productos</a></li>
        <li class="active">Ventas</li>
        <!------- Seccion del Author JIMERU ------>
        <li class="active">JJ</li>
    </ol>
</section>
<!--cuerpo de la pagina-->
<section class="content">
    <!------------- ventana de loader, permite bloquear la pagina mientras esta carga -------->
    <div id="overBody" class="loading" style="display:none;">
    </div>
    <div class="row">
        <!--carga de SKU (productos) existentes y button de comentarios-->
        <div class="col-md-4">
            <div class="box box-danger">
                <div class="box-header with-border">
                    <!----------------- titulo y link Agregar SKU ----------------->
                    <div class="row col-md-12">
                        <i class="fas fa-stream"></i>
                        <h3 class="box-title">Productos</h3>
                        <a class="btn btn-link pull-right bg-yellow" id="linkAddNuevoProducto"
                           href="javascript:void(0);" data-toggle="tooltip" data-placement="left"
                           title="Permite agregar una sugerencia de producto nuevo"><i class="fas fa-plus"></i>Agregar Nuevo SKU</a>
                    </div>
                </div>
                <div class="box-body">
                    <div class="col-md-12">
                        <div class="form-group">
                            <!-----------------   opciones de busqueda de SKU --------------------->
                            <h4 id="formBusqueda">Seleccione la forma de busqueda</h4>
                            <table id="tbRadios" class="table">
                                <thead class="bg-blue" data-toggle="tooltip" data-placement="right"
                                       title="Permite seleccionar el tipo de busqueda">
                                    <tr>
                                        <th scope="col">
                                            <input type="radio" name="buscar" id="radSKUExistente" value="1" checked> SKU Existentes
                                        </th>
                                        <th scope="col">
                                            <input type="radio" name="buscar" id="radSKUSugerido" value="2"> SKU Sugeridos
                                        </th>
                                    </tr>
                                </thead>
                            </table>
                            <!-----------------   Input de busqueda --------------------->
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <span class="glyphicon glyphicon-search"></span>
                                        </span>
                                        <input type="text" class="form-control" id="NombreProductoExistente" placeholder="Nombre / Codigo "
                                               style="text-transform:uppercase;" onkeyup="javascript:this.value=this.value.toUpperCase();">
                                        <input type="text" class="form-control" id="NombreProductoSugerido" placeholder="Nombre / Codigo "
                                               style="text-transform:uppercase;display:none;" onkeyup="javascript:this.value=this.value.toUpperCase();">
                                        <span class="input-group-btn">
                                            <button id="btnLimpiarInputBusqueda" class="btn btn-default"><i class="fas fa-times-circle"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <!--------------------  Button agregar actividad desde panel ---------->
                            <div id="btnActividadPrincipal" class="row" style="display:none;">
                                <div class="col-sm-12">
                                    <button type="button" onclick="AbrirModalActividad()" class="btn pull-right bg-yellow"
                                            data-toggle="tooltip" data-placement="left"
                                            title="Permite agregar una sugerencia o actividad sobre el producto seleccionado">
                                        Agregar sugerencia de producto <span></span><i class="fa fa-comment"></i>
                                    </button>
                                </div>
                            </div>
                            <input type="text" class="form-control" id="Producto" style="display:none;">
                            <br />
                            <!--dibuja las opciones de productos encontrados SKU Existentes-->
                            <div id="panelProductos" class="panel panel-default">
                                <div class="panel-heading">Productos encontrados </div>
                                <div id="panelFind">
                                    <table class="table table-bordered">
                                        <tbody id="contentProductos"></tbody>
                                    </table>
                                </div>
                            </div>
                            <!---------------------------------------------->
                            <!--dibuja las opciones de productos encontrados SKU Sugeridos-->
                            <div id="panelProductosSugeridos" class="panel panel-default" style="display:none">
                                <div class="panel-heading">Productos encontrados</div>
                                <div id="panelFind">
                                    <table class="table table-bordered">
                                        <tbody id="contentProductosSugeridos"></tbody>
                                    </table>
                                </div>
                            </div>
                            <!---------------------------------------------->
                            <br />
                            <!------------- button de fecha----------------->
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="glyphicon glyphicon-calendar"></i>
                                </div>
                                <input type="text" class="form-control pull-right" id="fechainicial">
                            </div>
                            <!---------------------------------------------->
                        </div>
                    </div>
                </div>
                <div class="box-footer">
                    <!---------------------- Button para visualizar comentarios de las actividades --------------->
                    <div class="col-md-12">
                        <div id="btn-Comentarios" class="btn btn-primary btn-block" data-toggle="tooltip" data-placement="right"
                             title="Permite buscar los comentarios, actividades o sugerencias sobre el producto seleccionado en un rango de fechas">
                            <i class="fa fa-comments"></i> Ver comentarios
                        </div>
                    </div>
                </div>
                <!------------- ventana de loader, permite bloquear la pagina mientras esta carga -------->
                <div id="over01" class="overlay" style="display:none;">
                    <i class="fa fa-refresh fa-spin"></i>
                </div>
            </div>
        </div>
        <!--carga de detalles del SKU -->
        <div class="col-md-8">
            <div class="box box-widget widget-user">
                <div class="widget-user-header bg-blue" style="height:175px;">
                    <h2 class="widget-user-username text-bold" id="lblCode"></h2>
                    <h5 class="widget-user-desc" id="lblArticulo"></h5>
                    <h5 class="widget-user-desc" id="lblFamilia"></h5>
                    <h5 class="widget-user-desc" id="lblSubFamilia" style="display:none"></h5>
                    <h5 class="widget-user-desc" id="lblEstatus"></h5>
                    <br />
                </div>
                <div class="box-footer">
                    <h5 class="widget-user-desc" id="lblEmpaque" style="display:none"></h5>
                    <h5 class="widget-user-desc" id="lblMarca" style="display:none"></h5>
                    <h5 class="widget-user-desc" id="lblRegistradoPor" style="display:none"></h5>
                    <h5 class="widget-user-desc" id="lblRegistradoEl" style="display:none"></h5>
                    <div id="tablaSKUSugerido" style="display:none">
                        <table id="tbSKUSugerido">
                            <thead>
                                <tr>
                                    <th>Tipo Precio</th>
                                    <th>Numero de piezas</th>
                                    <th>Precio por pieza</th>
                                </tr>
                            </thead>
                            <tbody id="TbContentSKUSugerido"></tbody>
                        </table>
                    </div>
                    <div class="widget-user-image" style="margin-top:15px;height:150px;">
                        <!--Carrusel de imagenes-->
                        <div class="row" id="carrousel-dinamic">
                        </div>
                    </div>
                </div>
                <!------------- ventana de loader, permite bloquear la pagina mientras esta carga -------->
                <div id="over02" class="overlay" style="display:none;">
                    <i class="fa fa-refresh fa-spin"></i>
                </div>
            </div>
        </div>
    </div>
    <!---------- Panel de conversaciones o comentarios del seguimiento de productos ---------->
    <div class="row" id="PanelSeguimientoProductos">
        <div class="col-md-12">
            <!------------------ button de opciones de panel de chat ------------------------->
            <ul class="nav nav-pills" role="tablist">
                <li role="presentation"><button id="btnSeguimientoSKU" class="btn btn-primary active">Seguimiento por SKU </button></li>
                @for (int i = 0; i < tam; i++)
                {
                    if (roles[i].ToString() == "Administrador" || roles[i].ToString() == "Direccion" || roles[i].ToString() == "Compras")
                    {
                        <li role="presentation"><button id="btnSeguimientoFecha" class="btn btn-primary">Seguimiento por fechas</button></li>
                        break;
                    }
                }
                <li role="presentation"><button id="btnSeguimientoSKUNuevos" class="btn btn-primary">Seguimiento de nuevos SKU </button></li>
            </ul>
            <!------------------------  Seguimiento de productos por SKU --------------------->
            <div class="box box-danger" id="SeguimientoSKU">
                <div class="box-header with-border">
                    <i class="fa fa-comments"></i>
                    <h3 class="box-title">Seguimiento de productos</h3>
                    @for (int i = 0; i < tam; i++)
                    {
                        if (roles[i].ToString() == "Administrador" || roles[i].ToString() == "Direccion" || roles[i].ToString() == "Compras")
                        {
                            <button type="button" id="btn-descargaReport" class="btn btn-xs pull-right btn-spacing bg-blue"
                                    data-toggle="modal" style="display:none;padding:5px; color:white; border-radius:5px;">
                                Descargar reporte <i class="glyphicon glyphicon-download" style="width:20px"></i>
                            </button>
                            break;
                        }
                    }
                    <button type="button" onclick="AbrirModalActividad()" class="btn btn-xs pull-right btn-spacing bg-blue" data-toggle="modal" style="border-radius:5px;height:30px;color:white;">Agregar actividad <span></span><i class="fa fa-comment" style="width:20px"></i></button>
                </div>
                <div class="box-body chat" id="chat-box">
                    <div class="row container-fluid">
                        <div id="treeview_json">
                            <!--aqui se genera el arbol de comentarios y respuestas-->
                        </div>
                    </div>
                </div>
                <div class="box-footer">
                </div>
                <!------------- ventana de loader, permite bloquear la pagina mientras esta carga -------->
                <div id="over03" class="overlay" style="display:none;">
                    <i class="fa fa-refresh fa-spin"></i>
                </div>
            </div>
            <!------------------------  Seguimiento de productos por fechas --------------------->
            <div class="box box-danger" id="SeguimientoFecha" style="display:none;">
                <div class="box-header with-border">
                    <form class="form-inline">
                        <i class="fa fa-comments"></i>
                        <h3 class="box-title">Seguimiento de productos por fecha</h3>
                        <div class="pull-right">
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </div>
                                    <input type="text" class="form-control pull-right" id="fechaSeguimiento">
                                </div>
                                <button type="button" id="btnBuscar" class="btn btn-primary"><i class="far fa-eye"></i> Ver</button>
                            </div>
                            <button type="button" id="btnDescargarReporteFechas" class="btn btn-primary" style="display:none;"> Descargar reporte <i class="glyphicon glyphicon-download" style="width:20px"></i></button>
                        </div>
                    </form>
                </div>
                <div class="box-body chat" id="chatSeguimientoFecha">
                    <div class="row container-fluid">
                        <div id="treeviewFechas">
                            <!--aqui se genera el arbol de comentarios y respuestas-->
                        </div>
                    </div>
                </div>
                <div class="box-footer">
                </div>
                <!------------- ventana de loader, permite bloquear la pagina mientras esta carga -------->
                <div id="over04" class="overlay" style="display:none;">
                    <i class="fa fa-refresh fa-spin"></i>
                </div>
            </div>
            <!------------------------  Seguimiento de productos nuevos por fecha de registro --------------------->
            <div class="box box-danger" id="SeguimientoSKUNuevoFecha" style="display:none;">
                <div class="box-header with-border">
                    <form class="form-inline">
                        <i class="fa fa-comments"></i>
                        <h3 class="box-title">Seguimiento de productos nuevos sugeridos</h3>
                        <div class="pull-right">
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </div>
                                    <input type="text" class="form-control pull-right" id="fechaSeguimientoNuevoSKU">
                                    <div class="input-group-btn">
                                        <button type="button" id="btnBuscarNuevoSKU" class="btn btn-primary"><i class="far fa-eye"></i> Ver</button>
                                    </div>
                                    <div class="input-group-btn">
                                        <button type="button" id="btnDescargarNuevoSKU" class="btn btn-primary" style="display:none;">
                                            Descargar Reporte
                                            <i class="glyphicon glyphicon-download"></i>
                                        </button>
                                    </div>
                                </div>

                            </div>

                        </div>
                    </form>
                </div>
                <div class="box-body chat" id="chatSeguimientoFechaNuevoSKU">
                    <div class="row container-fluid">
                        <div id="treeviewSKUFechasNuevo">
                            <!--aqui se genera el arbol de comentarios y respuestas-->
                        </div>
                    </div>
                </div>
                <div class="box-footer">
                </div>
                <!------------- ventana de loader, permite bloquear la pagina mientras esta carga -------->
                <div id="over05" class="overlay" style="display:none;">
                    <i class="fa fa-refresh fa-spin"></i>
                </div>
            </div>
        </div>
    </div>
    <!-------------------------------------------------------------------------------------------->
    <!--------- Modal de agregar actividad nueva(Nueva conversación(primer nivel)----------------->
    <div class="modal fade " id="modal-success">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header" style="background-color:orangered; color:azure">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title">Agregar actividad</h4>
                </div>
                <div class="modal-body">
                    <div class="box">
                        <form id="frm-actividad" method="post" class="form-horizontal" enctype="multipart/form-data">
                            <div class="box-body">
                                <div class="col-md-12">
                                    @for (int i = 0; i < tam; i++)
                                    {
                                        if (roles[i].ToString() == "Administrador" || roles[i].ToString() == "Direccion" || roles[i].ToString() == "Compras")
                                        {
                                            <div class="form-group">
                                                <label for="TxtPermisos" class="col-sm-4 col-sm-offset-4 control-label">Permisos de visualizacion</label>
                                                <div class="col-sm-4">
                                                    <select id="TxtPermisos" name="TxtPermisos" class="form-control" data-toggle="tooltip" data-placement="right"
                                                            title="Permite seleccionar quien puede ver esta actividad">
                                                        <option value="1" selected>Compras y Dirección</option>
                                                        <option value="2">Todos</option>
                                                    </select>
                                                </div>
                                            </div>
                                            break;
                                        }
                                        else if (roles[i].ToString() == "Ventas")
                                        {
                                            <select id="TxtPermisos" name="TxtPermisos" class="form-control" style="display:none">
                                                <option value="2" selected>Todos</option>
                                            </select>
                                            break;
                                        }
                                    }
                                    <!--------- columna izquierda ----------------------------------------------------->
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="txt-titulo" class="col-sm-2 control-label">Titulo</label>
                                            <div class="col-sm-10">
                                                <select id="txt-titulo" name="txt-titulo" class="form-control">
                                                    <option value="default" selected disabled>
                                                        Seleccione un título
                                                    </option>
                                                    @foreach (var item in this.Model.titulosActividads)
                                                    {
                                                        <option value="@item.Titulo">@item.Titulo.ToUpper()</option>
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class=" col-sm-12">
                                                <label for="txt-Comentario" class="control-label">Comentario o descripción</label>
                                                <textarea type="text" id="txt-Comentario" name="txt-Comentario" class="form-control"
                                                          placeholder="Agregar descripción de la actividad" data-toggle="tooltip" data-placement="right"
                                                          title="Agregue una sugerencia o comentario acerca del producto seleccionado" /></textarea>
                                            </div>
                                        </div>
                                    </div>
                                    <!--------- columna derecha ----------------------------------------------------->
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="id-Producto" class="col-sm-2 control-label">Producto</label>
                                            <div class="col-sm-10">
                                                <input type="text" class="form-control" id="id-Producto" name="id-Producto" readonly>
                                            </div>
                                        </div>
                                        <!---------------- Seleccion de imagenes ----------------->
                                        <div class="form-group">
                                            <div class="col-sm-12" style="margin-bottom:1px;">
                                                <label for="imagenEvidencia">Seleccione imagenes de evidencia</label>
                                                <input class="col-sm-12" type="file" id="imagenEvidencia" name="imagenEvidencia"
                                                       accept=".jpg, .jpeg, .png" multiple style="border:solid 0.5px;border-color:black;">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-sm-12">
                                                <div id="btn-addTable" class="btn btn-block bg-yellow" data-toggle="tooltip" data-placement="left"
                                                     title="Permite agregar datos a la table de comparación de precios">
                                                    <i class="fa fa-plus"></i> Agregar datos de comparación de precios
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row bg-red" style="text-align:center;">
                                <label class="text text-bold" style="color:white;"> Tabla de comparacion de precios  </label>
                            </div>
                            <!--manejo de table de comparaciones de precio-->
                            <div class="form-group" id="tb-Comparacion">
                                <table id="addTablePrecios" class="table" style="margin-left:5%;width:90%;">
