    //--------  carga los comentarios respecto a el codigo de producto y rango de fecha de comentarios  ---
    function Comentarios() {
        //aqui se genera la informacion acerca de los comentarios
        //o refierente al armado de arbol de actividad
        var Producto = $("#Producto").val();
        var drp = $('#fechainicial').data('daterangepicker');
        var Del = drp.startDate.format('YYYY/MM/DD');
        var Al = drp.endDate.format('YYYY/MM/DD');
        $.ajax({
            contentType: "application/json; charset=utf-8",
            type: 'POST',
            url: '@Url.Action("GetActividadesProductos", "ActividadesProductos")',
            async: false,
            data: '{ Producto: "' + Producto + '", Del: "' + Del + '", Al: "' + Al + '"}',
            dataType: 'json',
            success: function (response) {
                //procesar para dibujar estructura de los comentrarios treeview
                initTree(response)
            }
        });
        function initTree(treeData) {
            $('#treeview_json').treeview({
                data: treeData.Context,
            });
            contraeTree();
            $(".esconder").hide();
        }
    }
    function contraeTree() {
        $('#treeview_json').treeview('collapseAll', { silent: true });
    }
