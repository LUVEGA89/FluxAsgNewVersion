<script>
    FechaSeguimientoNuevoSKU();
    function FechaSeguimientoNuevoSKU() {
        $('#fechaSeguimientoNuevoSKU').daterangepicker();
        $.fn.datepicker.defaults.format = "dd/mm/yyyy";
    }
    function NuevosSKUXFechas() {
        ShowOver();
        var drp = $('#fechaSeguimientoNuevoSKU').data('daterangepicker');
        var Del = drp.startDate.format('YYYY-MM-DD');
        var Al = drp.endDate.format('YYYY-MM-DD');
        $.ajax({
            contentType: "application/json; charset=utf-8",
            type: 'POST',
            url: '@Url.Action("GetNuevoSKUPorFechas", "ActividadesProductos")',
            async: false,
            data: '{Del: "' + Del + '", Al: "' + Al + '"}',
            dataType: 'json',
            success: function (response) {
                //procesar para dibujar estructura de los comentrarios treeview
                initTree(response)
                HideOver()
            },
            error: function () {
                HideOver();
            }
        });
        function initTree(treeData) {

            $('#treeviewSKUFechasNuevo').treeview({
                data: treeData.Context,
            });
            $('#treeviewSKUFechasNuevo').treeview('collapseAll', { silent: true });
        }
    }
    $("#btnBuscarNuevoSKU").click(function () {
        NuevosSKUXFechas();
        $("#btnDescargarNuevoSKU").show();
    });
    $("#btnDescargarNuevoSKU").click(function () {
        var drp = $('#fechaSeguimientoNuevoSKU').data('daterangepicker');
        var Del = drp.startDate.format('YYYY/MM/DD');
        var Al = drp.endDate.format('YYYY/MM/DD');
        $.ajax({
            contentType: "application/json; charset=utf-8",
            type: 'POST',
            url: '@Url.Action("seguimientoXlsNuevoSKU", "ActividadesProductos")',
            async: false,
            data: '{Del: "' + Del + '", Al: "' + Al +'"}',
            dataType: 'json',
            success: function (data) {
                window.location.href = '@Url.Action("DownloadSeguimientoXls", "ActividadesProductos")' +
                    '?fileGuid=' + data.FileGuid + '&fileName=' + data.FileName;
            }
        });
    });
</script>