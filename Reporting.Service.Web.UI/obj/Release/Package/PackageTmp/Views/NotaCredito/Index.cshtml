                    mostrarTablasFacturaOrigenInventario();
                    //API GetFacturaNC
                    consultaNotaCreditoOrigen(cadena_completa_cliente, factura_origen);
                    //API GetFacturaDestino
                    consultaFacturaDestino(factura_destino, cadena_completa_cliente, concepto_descuento);
                    //API GetFacturaNCValidacionLimiteDinero
                    getFacturaNCValidacionLimiteDinero(cadena_completa_cliente, factura_destino, factura_origen);
