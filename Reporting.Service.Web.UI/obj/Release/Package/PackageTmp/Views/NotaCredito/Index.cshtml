    function GetNotaCreditoInventario() {
        // folio de la factura origen
        let FOrigen = $('#input-factura-origen').val();
        let count = 0;

        $.ajax({
            contentType: "application/json; charset=utf-8",
            url: '@Url.Action("GetNotaCreditoInventario", "NotaCredito")',
            async: false,
            timeout: 3000,
            type: 'POST',
            data: '{"FacturaOrigen":"' + FOrigen + '","TipoDocumento":"I"}',
            dataType: "json",
            success: function (data) {

                if (data != null && $.isArray(data.Context)) {

                    // "#table-invoice-history"
                    var VP = $('#factura-origen').DataTable();
                    VP.destroy();
                    $("#table-invoice-history").html('');

                    count = data.Context.length;

                    //console.log("Notas de credito encontradas " + count);

                    $.each(data.Context, function (index, value) {
                        let EstadoAprobacion = '';

                        if (value.TipoDocumento == 'S') {
                            if (value.Valor == 1 && value.Canal == 'RETAIL') {

                                EstadoAprobacion = 'Aprobación pendiente por credito y cobranza';
                            }
                            else {
                                EstadoAprobacion = 'Aprobación pendiente por dirección';
                            }
                            value.TipoDocumento = 'Servicio';
                        }

                        if (value.TipoDocumento == 'I') {
                            value.TipoDocumento = 'Inventario';
                            if (value.Estatus == 1) {
                                value.Estatus = 'Aprobación pendiente por el almacén'
                            }
                        }

                        if (value.Valor == 0) {
                            if (value.ConceptoDescuento == 39) {

                                EstadoAprobacion = 'Aprobación pendiente por credito y cobranza';
                            }
                            else {
                                EstadoAprobacion = 'Aprobación pendiente por gerencia';
                            }
                        }

                        if (value.Valor == 2) {
                            EstadoAprobacion = 'Aprobación pendiente por la dirección'
                        }
                        if (value.Valor == 3) {
                            EstadoAprobacion = 'Autorizada'
                        }
                        if (value.Valor == 4) {
                            EstadoAprobacion = 'SAP'
                        }
                        if (value.Valor == 5) {
                            EstadoAprobacion = 'Rechazada';
                        }
                        if (value.Valor == 10) {
                            EstadoAprobacion = 'No especificado';
                        }
                        $("#table-invoice-history").append(
                            "<tr>\
                                <td>" + value.Identifier + "</td>\
                                <td>" + value.ClienteName + "</td>\
                                <td>" + value.FolioOrigen + "</td>\
                                <td>" + value.FolioDestino + "</td>\
                                <td>" + value.TipoDocumento + "</td>\
                                <td>" + value.ConceptoDescuentoDetalle + "</td>\
                                <td>" + value.Comentario + "</td>\
                                <td>" + value.Usuario + "</td>\
                                <td>" + EstadoAprobacion + "</td>\
                                <td>" + moment(value.Fecha).format("YYYY-MM-DD HH:mm:ss") + "</td>\
                                <td>" + value.FolioSap + "</td>\
                                <td>" + value.FolioPagoSAP + "</td>\
                                <td align='center'>\
                                    <a class='btn btn-primary' data-target='#modal-nota-credito-inventario' data-toggle='modal' onclick='getHistoricoNotaCreditoDetail("+ value.Identifier + ")' href= '#'>\
                                        <i class='fa fa-file-text'></i>\
                                    </a>\
                                </td>\
                                <td align='center'>\
                                    <a class='btn btn-info' data-target='#verComentarios' data-toggle='modal' onclick='verComentarios("+ value.Identifier + ")' data-id='" + value.Identifier + "' href= '#'>\
                                        <i class='fa fa-comment'></i>\
                                    </a>\
                                </td>\
                                <td align='center'>\
                                    <a class='btn btn-info' data-target='#verImagenes' data-toggle='modal' onclick='verImagenesNC("+ value.Identifier + ")' data-id='" + value.Identifier + "' href= '#'>\
                                        <i class='fa fa-eye'></i>\
                                    </a>\
                                </td>\
                                </tr>");
                    });

                    $('#factura-origen').DataTable({
