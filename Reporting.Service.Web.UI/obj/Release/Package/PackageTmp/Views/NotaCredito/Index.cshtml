                            <div class="form-group">
                                <label class="col-sm-2 control-label">Monto destino</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" id="input-factura-destino-monto-permitido-servicio-otro-descuento" disabled />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 control-label">Monto solicitado sin IVA</label>
                                <div class="col-sm-10">
                                    <!--<input type="text" class="form-control number-only" id="input-factura-destino-monto-solicitado-servicio-otro-descuento-formato" onkeyup="validarCantidadPermitidaNotaCreditoServicioOtroDescuento()">-->
