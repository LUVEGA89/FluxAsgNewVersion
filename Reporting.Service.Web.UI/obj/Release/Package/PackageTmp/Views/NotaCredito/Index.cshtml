                    <i class="fa fa-credit-card"></i>
                    <h3 class="box-title">Nota de credito</h3>
                    <div id="loading" style="display:none;" class="overlay">
                        <i class="fa fa fa-refresh fa-spin"></i>
                    </div>
                </div>
                <div class="box-body">
                    <div class="table table-responsive">
                        <!-- TABLA FACTURA ORIGEN INVENTARIO -->
                        <table class="table table-bordered" id="tabla-factura-origen-inventario" style="width: 60%;">
                            <thead>
                                <tr>
                                    <th class="th-stationery" style="width: 20%;">Sku</th>
                                    <th class="th-stationery" style="width: 10%;">Solicitar</th>
                                    <th class="th-stationery" style="width: 10%;">Cantidad</th>
                                    <th class="th-stationery" style="width: 20%;">Precio</th>
                                </tr>
                            </thead>
                            <tbody id="tbody-factura-origen-inventario"></tbody>
