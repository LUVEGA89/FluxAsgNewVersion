    function getHistoricoNotaCreditoDetail(FolioNotaCredito) {
        resetFormsGestion();

        $(".main-timeline-section").show();
        $("#estatus-rechazado").text("");

        $.ajax({
            contentType: "application/json; charset=utf-8",
            url: '@Url.Action("GetNotaCreditoItem", "NotaCredito")',
            async: false,
            timeout: 3000,
            type: 'POST',
            data: '{"NotaCredito":"' + FolioNotaCredito + '"}',
            dataType: "json",
            success: function (data) {
                if (data != null) {

                    let FechaFormato = moment(data.Context.Fecha).format("YYYY-MM-DD HH:mm:ss");
                    let ConceptoDescuento = data.Context.ConceptoDescuento;

                    let TypeDoc = data.Context.TipoDocumento;
                    if (TypeDoc == 'S') {
                        TypeDoc = 'Servicio';
                    }
                    if (TypeDoc == 'I') {
                        TypeDoc = 'Inventario';
                    }
                    let canal = data.Context.Canal;
                    //if (ConceptoDescuento != 38 && ConceptoDescuento != 39) {
                    //    $(".line-time-change-text").text("");
                    //    $(".line-time-change").text("");
                    //    $(".gestor-timeline-change").css("visibility", "hidden");
                    //}
                    let Total = 0;
                    $("#table-invoice-detail").empty();

                    $.each(data.Context.Items, function (index, value) {
                        let SubTotalDetail = 0;
                        let Descuento = 0;
                        switch (data.Context.TipoDocumento) {
                            case 'I':
                                SubTotalDetail = value.Cantidad * value.Precio;
                                Descuento = SubTotalDetail;
                                break;
                            case 'S':
                                SubTotalDetail = value.Cantidad * value.Descuento;
                                Descuento = value.Descuento;
                                break;
                            default:
                        }
                        if (ConceptoDescuento == 37) {
                            SubTotalDetail = value.Cantidad * value.Descuento;
                            Descuento = value.Descuento;
                        }
                        Total = Total + SubTotalDetail;

                        $("#table-invoice-detail").append(
                            "<tr>\
                                <td>" + value.ItemCode + "</td>\
                                <td align='center'>" + value.Cantidad + "</td >\
                                <td align='center'>" + formatNumber.new(value.Precio.toFixed(2), "$ ") + "</td>\
                                <td style='display:none'><input type='text' value='" + SubTotalDetail + "'/></td>\
                                <td align='center'>" + formatNumber.new(value.Descuento.toFixed(2), "$ ") + "</td>\
                                <td align='center' style='display:none'><input type='text' class='sub-total-detail' value='" + SubTotalDetail + "'/></td>\
                                <td align='center'>" + formatNumber.new(SubTotalDetail.toFixed(2), "$ ") + "</td>\
                            </tr>");

                    });
                    $('#total-detail').text(formatNumber.new(Total.toFixed(2), "$ "));

                    let Estatus = data.Context.Valor;
                    switch (Estatus) {
                        case 0:
                            $("#estatus-gerencia").addClass('meta-date');
                            $("#estatus-almacen").addClass('meta-date');
                            $("#estatus-credito").addClass('meta-date');
                            $("#estatus-direccion").addClass('meta-date');
                            $('.timeline-end').addClass('meta-date')
                            break;
                        case 1:
                            $("#estatus-gerencia").addClass('meta-date-gestion');
                            //$("#estatus-almacen").addClass('meta-date');
                            //$("#estatus-credito").addClass('meta-date');
                            //$("#estatus-direccion").addClass('meta-date');
                            $('.timeline-end').addClass('meta-date')
                            break;
                        case 2:
                            $("#estatus-gerencia").addClass('meta-date-gestion');
                            $("#estatus-almacen").addClass('meta-date-gestion');
                            $("#estatus-credito").addClass('meta-date-gestion');
                            //$("#estatus-direccion").addClass('meta-date');
                            $('.timeline-end').addClass('meta-date')
                            break;
                        case 3:
                            $("#estatus-gerencia").addClass('meta-date-gestion');
                            $("#estatus-almacen").addClass('meta-date-gestion');
                            $("#estatus-credito").addClass('meta-date-gestion');
                            $("#estatus-direccion").addClass('meta-date-gestion');
                            $('.timeline-end').addClass('meta-date')
                            break;
                        case 4:
                            $("#estatus-gerencia").addClass('meta-date-gestion');
                            $("#estatus-almacen").addClass('meta-date-gestion');
                            $("#estatus-credito").addClass('meta-date-gestion');
                            $("#estatus-direccion").addClass('meta-date-gestion');
                            $('.timeline-end').addClass('meta-date-gestion')
                            break;
                        case 5:
                            $(".main-timeline-section").hide();
                            $("#estatus-rechazado").text("NOTA DE CREDITO RECHAZADA");
                            break;
                    }

                    if (ConceptoDescuento == 37) { // diferencia en precio 100%

                        $("#estatus-gerencia").addClass('meta-date-gestion');
                        $("#estatus-direccion").addClass('meta-date-gestion');

                        $('#div-gestion-gerencia').show();
                        $('#estatus-gerencia').show();

                    }

                    else if (ConceptoDescuento == 38) { // inventario
                        $('#estatus-gerencia').show();
                        $('#div-gestion-gerencia').show();

                        $('#estatus-almacen').show();
                        $('#div-gestion-almacen').show();
                    }

                    else if (ConceptoDescuento == 39) { // pronto pago
                        $('#div-gestion-credito').show();
                        $('#estatus-credito').show();

                    }
                    else if (canal == 'RETAIL' && data.Context.TipoDocumento != 'I' && ConceptoDescuento != 39 && ConceptoDescuento != 38 && ConceptoDescuento != 37) {
                        $('#div-gestion-credito').show();
                        $('#div-gestion-gerencia').show();
                        $('#estatus-gerencia').show();
                        $('#estatus-credito').show();

                        $('#div-gestion-almacen').hide();
                        $('#estatus-almacen').hide();
                    }
                    else {
                        $('#estatus-gerencia').show();
                        $('#div-gestion-gerencia').show();
                    }

                    $("#set-Sequence").text("FOLIO SIE: " + data.Context.Identifier);
                    $("#set-Cliente").text("CLIENTE: " + data.Context.ClienteName);
                    $("#set-FolioOrigen").text("FOLIO ORIGEN: " + data.Context.FolioOrigen);
                    $("#set-FolioDestino").text("FOLIO DESTINO: " + data.Context.FolioDestino);
                    $("#set-Comentario").text("COMENTARIO: " + data.Context.Comentario);
                    $("#set-Usuario").text("USUARIO: " + data.Context.Usuario);
                    $("#set-TipoDocumento").text("TIPO DOCUMENTO: " + TypeDoc);
                    $("#set-Fecha").text("FECHA: " + FechaFormato);

                }
            }, error: function (data) {
                swal("Error en la api: GetNotaCreditoItem: " + data);
            }
        });
    }

