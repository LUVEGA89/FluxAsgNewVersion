    //CALCULA MONTO PERMITIDO NOTA DE CREDITO SERVICIO DESCUENTO
    function calculaMontoPermitidoNotaCreditoServicioDescuento() {
        var sumsd = 0;
        $(".subtotal").each(function () {
            let sumadosd = this.value;
            if (!isNaN(sumadosd) && sumadosd.length != 0) {
                sumsd += parseFloat(sumadosd);
            }
        });
        let totalsd = sumsd * 1.16;
        let ivasd = sumsd * 0.16;
        let subtotalsd = sumsd;
        $('#iva-servicio-descuento').text("Iva: " + formatNumber.new(ivasd.toFixed(2), "$ "));
        $('#subtotal-servicio-descuento').text("Subtotal: " + formatNumber.new(subtotalsd.toFixed(2), "$ "));
        $('#input-factura-destino-monto-solicitado-servicio-descuento').val(subtotalsd);
        $('#total-servicio-descuento').text("Total: " + formatNumber.new(totalsd.toFixed(2), "$ "));
        let monto_permitidosd = $("#input-factura-destino-monto-disponible-servicio-descuento").val();
        let monto_solicitadosd = $('#input-factura-destino-monto-solicitado-servicio-descuento').val();
        let factura_destino_monto_permitidosd = parseFloat(monto_permitidosd);
        let factura_destino_monto_solicitadosd = parseFloat(monto_solicitadosd).toFixed(2);
        if (factura_destino_monto_solicitadosd > factura_destino_monto_permitidosd) {
            swal({
                title: "Solicitud",
                text: "El monto de la nota de credito original no puede ser superado.",
                icon: "error",
                button: "OK",
            });
            $('#row' + countsd).remove();
            //CALCULA MONTO PERMITIDO NOTA DE CREDITO SERVICIO DESCUENTO
            //calculaMontoPermitidoNotaCreditoServicioDescuento();
        } else if (factura_destino_monto_solicitadosd == factura_destino_monto_permitidosd) {
            swal({
                title: "Solicitud",
                text: "Nota de credito (inventario) completada.",
                icon: "success",
                button: "OK",
            });
        }
    };
