                                <div class="col-xs-12 table-responsive">
                                    <table class="table table-striped">
                                        <thead>
                                            <tr>
                                                <th class="th-stationery">Sku</th>
                                                <th class="th-stationery">Cantidad</th>
                                                <th class="th-stationery" id="modal-precio">Precio</th>
                                                <th class="th-stationery" id="modal-descuento">Descuento</th>
                                                <th class="th-stationery" id="modal-descuento-total">Descuento total sin IVA</th>
                                            </tr>
                                        </thead>
                                        <tbody id="table-invoice-detail"></tbody>
                                        <tfoot align="right">

                                            <tr style="text-align:right">
                                                <td colspan="4">
                                                    <b>Total sin IVA:</b>
                                                </td>
                                                <td><span id="total-detail"></span></td>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <h1 id="estatus-rechazado"></h1>
                        <section class="main-timeline-section">
                            <div class="timeline-start"></div>
                            <div class="conference-center-line"></div>
                            <div class="conference-timeline-content">

                                <div class="timeline-article timeline-article-bottom">
                                    <div id="div-gestion-direccion" class="content-date">
                                        <small>Aprobación</small><br>
                                        <small>dirección</small>
                                    </div>
                                    <div class="meta-date" id="estatus-direccion"></div>
                                </div>

                                <div class="timeline-article timeline-article-bottom">
                                    <div id="div-gestion-credito" class="content-date">
                                        <small>Aprobación</small><br />
                                        <small>credito</small>
                                    </div>
                                    <div class="meta-date" id="estatus-credito"></div>
                                </div>

                                <div class="timeline-article timeline-article-bottom gestor-timeline-change">
                                    <div id="div-gestion-almacen" class="content-date">
                                        <small>Aprobación</small><br>
                                        <small>almacén</small>
                                    </div>
                                    <div class="meta-date" id="estatus-almacen"></div>
                                </div>

                                <div class="timeline-article timeline-article-bottom">
                                    <div id="div-gestion-gerencia" class="content-date">
                                        <small>Aprobación</small><br>
                                        <small>gerencia</small>
                                    </div>
                                    <div class="meta-date" id="estatus-gerencia"></div>
                                </div>

                            </div>

                            <div class="timeline-end"></div>
                        </section>
                        <br>
                        <br>
                        <hr>
                        <button type="button" class="btn btn-info pull-right" onclick="javascript:genScreenshot();"><i class="fa fa-fw fa-cloud-download"></i></button>
                        <a id="test"></a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- MODAL COMENTARIOS SOBRE LA NOTA DE CREDITO -->
    <div id="verComentarios" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-body">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-xs-12">
                                <h2 class="page-header">
                                    <i class="fa fa-file-text"></i>&nbsp;&nbsp;&nbsp;Comentarios nota de crédito
                                    <small class="pull-right" id="set-comentario"></small>
                                </h2>
                            </div>
                        </div>
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th class="th-stationery">Departamento</th>
                                    <th class="th-stationery">Usuario</th>
                                    <th class="th-stationery">Comentario</th>
                                </tr>
                            </thead>
                            <tbody id="tbody-comentarios"></tbody>
                        </table>
                    </div>
                    <form role="form">
                        <input id="Identifier_Comentario" type="text" style="display:none;" />
                        <div class="box-body">
                            <div class="form-group">
                                <label>Comentario </label>
                                <input type="text" multiple class="form-control" id="text-comentario" placeholder="Comentario">
                            </div>
                        </div>
                        <div class="box-footer">
                            <button id="btn-cancel-comentario" data-dismiss="modal" type="button" class="btn btn-default">Cancel</button>
                            <button id="btn-add-comentario" data-dismiss="modal" type="button"  class="btn btn-info pull-right">Guardar</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <div id="verImagenes" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-body">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-xs-12">
                                <h2 class="page-header">
                                    <i class="fa fa-file-text"></i>&nbsp;&nbsp;&nbsp;Evidencias de nota de crédito
                                </h2>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-sm-6 invoice-col">
                                <address>
                                    <small id="set-FolioNC"></small><br>
                                    <small id="set-ClienteNC"></small><br>
                                </address>
                            </div>
                            <div class="col-sm-6 invoice-col">
                                <address>
                                    <small id="set-FolioOrigenNC"></small><br>
                                    <small id="set-FolioDestinoNC"></small><br>
                                    <small id="set-TipoDocumentoNC"></small><br>
                                </address>
                            </div>
                        </div>
                        <div class="row">
                            <div class="box box-widget widget-user">
                                <div class="widget-user-image">
                                    <!--<img class="img-circle" src="../dist/img/boxed-bg.jpg" alt="User Avatar">-->


                                </div>
                                <div class="widget-user-header bg-blue">

                                    <div class="row" id="carrousel-dinamic">

                                    </div>
                                </div>

                            </div>
                        </div>

                    </div>

                    <form id="frm-addImagenes" method="post" class="form-horizontal" enctype="multipart/form-data">
                        <div class="box-body">
                            <div class="form-group">
                                <div class="row">
                                    <div class="box-body">
                                        <h2 class="page-header">
                                            <i class="fa fa-file-text"></i>&nbsp;&nbsp;&nbsp;Agregar evidencia
                                            <input type="hidden" id="NotaCreditoId" name="NotaCreditoId">
                                            <br />
                                            <small style="color:red !important">*Nota: Archivos admitido (Imagen y archivo PDF) con un tamaño maxico de 2 MB</small>
                                        </h2>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-12">
                            <div class="form-group">
