    //CALCULA MONTO PERMITIDO NOTA DE CREDITO INVENTARIO
    function calculaMontoPermitidoNotaCreditoInventario() {
        var sum = 0;
        $(".subtotal-hidden").each(function () {
            let sumado = parseFloat(this.value);
            if (!isNaN(sumado) && sumado.length != 0) {
                sum += sumado;
            }
        });
        let subtotal = sum;
        let iva = sum * 0.16;
        let total = sum * 1.16;
        $('#iva-inventario').text("Iva: " + formatNumber.new(iva.toFixed(3), "$ "));
        $('#subtotal-inventario').text("Subtotal: " + formatNumber.new(subtotal.toFixed(3), "$ "));
        $('#input-factura-destino-monto-solicitado').val(total);
        $('#total-inventario').text("Total: " + formatNumber.new(total.toFixed(3), "$ "));
        let monto_permitido = $("#input-factura-destino-monto-disponible").val();
        let monto_solicitado = $('#input-factura-destino-monto-solicitado').val();
        let factura_destino_monto_permitido = parseFloat(monto_permitido);
        let factura_destino_monto_solicitado = parseFloat(monto_solicitado);

        if (factura_destino_monto_solicitado > (factura_destino_monto_permitido + 300)) {
            swal({
                title: "Solicitud",
                text: "El monto de la nota de credito original no puede ser superado.",
                icon: "error",
                button: "OK",
            });
            $('#row' + count).remove();
            //CALCULA MONTO PERMITIDO NOTA DE CREDITO INVENTARIO
            calculaMontoPermitidoNotaCreditoInventario();
        } else if (factura_destino_monto_solicitado == factura_destino_monto_permitido) {
            swal({
                title: "Solicitud",
                text: "Nota de credito (inventario) completada.",
                icon: "success",
                button: "OK",
            });
        }
    };
    //API GetFacturaNCValidacionLimiteDinero
    function getFacturaNCValidacionLimiteDinero(cadena_completa_cliente,factura_destino,factura_origen) {
        $.ajax({
            contentType: "application/json; charset=utf-8",
            url: '@Url.Action("GetFacturaNCValidacionLimiteDinero", "Venta")',
            async: false,
            timeout: 3000,
            type: 'POST',
            data: '{"CardCode":"' + cadena_completa_cliente + '","FolioDestino":"' + factura_destino + '","FolioOrigen":"' + factura_origen + '"}',
            dataType: "json",
            success: function (data) {
                if (data != null && $.isArray(data.Context)) {
                    let Origen = $("#input-factura-destino-monto-permitido").val();
                    // tipo de descuento
                    let Subtotal = data.Context[0].Subtotal;
                    let Resta = Origen - Subtotal;
                    if (Resta <= 0) {
                        Resta = 0.00;
                    }
                    let canal = data.Context[0].Canal;

                    $("#input-factura-destino-monto-disponible").val(Resta.toFixed(2));
                    $(".span-help-block-destino-monto-ocupado").text("Monto ocupado: " + formatNumber.new(Subtotal.toFixed(2), "$ "));
                    $(".span-help-block-destino-monto-disponible").text("Monto disponible: " + formatNumber.new(Resta.toFixed(2), "$ "));
                }
            },
            error: function () {
                alert("Error en la api: GetFacturaNCValidacionLimiteDinero");
            }
        });
    }
