    //CLONA EN TABLA VALORES DE LA NOTA DE CREDITO TIPO INVENTARIO
    function clonarTablaNotaDeCreditoInventario(Codigo, Precio, index, AcctCode, Cantidad) {

        let exist = 0;
        // count list
        let count = $(".sku_inventario");

        if (count.length > 0) {

            var aux = 0;
            $(".sku_inventario").each(function () {
                let sku = this.value;

                let cant1 = $("#cantidad-inventario-" + aux).val();

                if (Codigo == sku && cant1 == Cantidad) {
                    exist = 1;
                    swal("Registro duplicado", "El SKU " + Codigo + " ya se ha agregado anteriormente.", "warning");
                }
                aux++;
            });
        }
        if (exist == 0) {
            $("#tr-invoice-" + index).css("background-color", "#e6ffe6");
            $('#boton-cancelar-nota-credito-inventario').show();
            $('#boton-solicitar-nota-credito-inventario').show();
            $("#tbody-nota-credito-solicitada-inventario").show();
            agregarNotaCreditoInventario(Codigo, Precio);
            let Cantidad = $("#cantidad-inventario-" + index).val();
            let Cant = parseInt(Cantidad);
            let Pre = parseFloat(Precio);
            let SubTotal = Cant * Pre;
            let Sub = SubTotal;
            let subFormat = formatNumber.new(Sub.toFixed(3), "$ ");
            let preFormat = formatNumber.new(Pre.toFixed(3), "$ ");
            count1 = count1 + 1;
            var html_code = "";
            html_code += "<tr id='row" + count1 + "'>";
            html_code += "<td><input class='sku_inventario' type='text' name='itemcode[]' value='" + Codigo + "' class='itemcode-inventario' disabled/></td>";
            html_code += "<td><input type='text' name='cantidad[]' value='" + Cant + "' class='cantidad-inventario' disabled/></td>";
            html_code += "<td><input type='text' value='" + preFormat + "' name='pre-format[]' class='precio-inventario' disabled/></td>";
            html_code += "<td style='display:none'><input type='hidden' name='precio[]'   value='" + Pre + "'/></td>";
            html_code += "<td><input type='text' class='subtotal subtotal-inventario' name='sub-format[]' value='" + subFormat + "' disabled/></td>";
            html_code += "<td style='display:none'><input type='hidden' class='subtotal-hidden' value='" + Sub + "'/></td>";
            html_code += "<td style='display:none'><input type='text' name='acctcode[]' value='" + AcctCode + "'/></td>";
            html_code += "<td><button type='button' name='remove' data-row='row" + count1 + "' onclick='test(" + index + ")' class='btn btn-danger btn-sm remove'><i class='fa fa-trash'></i></button></td>";
