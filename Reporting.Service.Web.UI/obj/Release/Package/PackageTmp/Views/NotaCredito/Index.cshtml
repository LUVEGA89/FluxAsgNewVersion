@model Reporting.Service.Web.UI.Models.UserModel
@using Microsoft.AspNet.Identity
@using Reporting.Service.Core.Common
@{
    ViewBag.Title = "Notas de Credito";
}

<style>
    .widget-user .box-footer {
        padding-top: 100px !Important;
    }

    .carousel-inner > .item > a > img, .carousel-inner > .item > img, .img-responsive, .thumbnail a > img, .thumbnail > img {
        display: block;
        width: 100%;
        height: 250px;
    }

    .btn-spacing {
        margin-right: 10px;
    }

    #estatus-rechazado {
        text-align: center;
        color: red;
    }

    #select-concepto-descuento option[value="37"] {
        background: yellow;
    }

    #select-concepto-descuento option[value="39"] {
        background: yellow;
    }

    .btn-spacing {
        margin-right: 10px;
    }

    .widget-user .widget-user-header {
        padding: 10px;
        height: 280px;
        border-top-right-radius: 3px;
        border-top-left-radius: 3px;
    }

    .carousel-caption {
        position: absolute;
        right: 0;
        padding-top: 20px;
        padding-bottom: 20px;
        color: #fff;
        text-align: center;
        bottom: 0;
        left: 0;
        z-index: 10;
        background: rgba(0,0,0,0.4);
    }

    .meta-date-gestion {
        background: #F2635F !important
    }
</style>
<section class="content-header">
    <h1>
        Nota de Crédito Solicitud
        <small id="span-cliente"></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Nota de Crédito</a></li>
        <li class="active">Solicitud</li>
    </ol>
</section>
<section class="content">
    <div class="row">
        <div class="col-md-4" id="cabecera">
            <div class="box box-danger">
                <div class="box-header">
                    <i class="fa fa-search"></i>
                    <h3 class="box-title">Motór de busqueda</h3>
                </div>
                <!-- FORMULARIO CABECERA -->
                <form role="form">
                    <div class="box-body">

                        <div class="form-group">
                            <label for="">Cliente</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <span class="glyphicon glyphicon-search"></span>
                                </div>
                                <input type="text" class="form-control" id="input-cliente" placeholder="Nombre / Codigo" data-behavior="autocomplete" value="" required>
                            </div>
                        </div>

                        <div class="form-group" id="div-custom-origen">
                            <label for="">Factura origen</label>
                            <input class="form-control" id="input-factura-origen" type="text" value="" required>
                            <span class="help-block span-help-block-origen"></span>
                        </div>

                        <div class="form-group" id="div-custom-destino">
                            <label>Factura destino</label>
                            <input class="form-control" id="input-factura-destino" type="text" value="" required>
                            <!-- INVENTARIO -->
                            <input id="input-factura-destino-monto-permitido" type="hidden">
                            <input id="input-factura-destino-monto-solicitado" type="hidden">
                            <input id="input-factura-destino-monto-disponible" type="hidden">
                            <!-- SOLICITUD SERVICIO -->
                            <input id="input-factura-destino-monto-permitido-servicio-descuento" type="hidden">
                            <input id="input-factura-destino-monto-solicitado-servicio-descuento" type="hidden">
                            <input id="input-factura-destino-monto-disponible-servicio-descuento" type="hidden">
                            <input id="input-factura-destino-cuenta-servicio-descuento-hidden" type="hidden">
                            <!-- SOLICITUD OTRO SERVICIO -->
                            <input type="text" style="display:none;" id="input-factura-destino-monto-permitido-porcentaje-servicio-otro-descuento-hidden">
                            <input type="text" style="display:none;" id="input-factura-destino-monto-permitido-servicio-otro-descuento-hidden">
                            <input type="hidden" id="input-factura-destino-monto-solicitado-servicio-otro-descuento-hidden">
                            <input type="hidden" id="input-factura-destino-cuenta-servicio-otro-descuento-hidden">



                            <span class="help-block span-help-block-monto-destino"></span>
                            <span class="help-block span-help-block-destino-monto-ocupado"></span>
                            <span class="help-block span-help-block-destino-monto-disponible"></span>
                        </div>

                        <div class="form-group">
                            <label>Tipo de documento</label>
                            <select class="form-control" id="select-tipo-documento">
                                <option value="O" selected="selected">Seleccionar...</option>
                                <option value="I">Inventario</option>
                                <option value="S">Servicio</option>
                            </select>
                        </div>

                        <div class="form-group" id="div-concepto-descuento">
                            <label>Concepto de descuento</label>
                            <select class="form-control" id="select-concepto-descuento"></select>
                        </div>

                        <div class="form-group">
                            <label for="">Comentario</label>
                            <textarea class="form-control" rows="2" id="comentario"></textarea>
                        </div>

                        <div id="loading-sap" style="display:none;" class="overlay">
                            <i class="fa  fa fa-refresh fa-spin"></i>
                        </div>
                        <button type="button" class="btn btn-info" id="btn-sap

">&nbsp&nbsp&nbsp<i class="fa fa-rocket">&nbsp&nbsp&nbsp</i></button>
                        <button type="button" class="btn btn-success pull-right" id="btn-enviar-cabeza" style="display:none">&nbsp&nbsp&nbsp<i class="fa fa-search">&nbsp&nbsp&nbsp</i></button>
                    </div>
                </form>
            </div>
        </div>
        <div class="col-md-8">
