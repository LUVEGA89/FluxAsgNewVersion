                        dom: 'Bfrtip',
                        stateSave: true,
                        buttons: [
                            'excel'
                        ],
                        order: [[1, 'desc']],
                        "paging": true,
                        "searching": true,
                        "language": {
                            "url": "//cdn.datatables.net/plug-ins/1.10.13/i18n/Spanish.json"
                        }
                    });
                }
                if (count > 0) {
                    swal("Verificar notas de credito aplicadas en el historial a la factura actual: " + FOrigen + "");
                }

            },
            error: function () {
                swal("Error en la api: GetNotaCreditoInventario");
            }
        });
